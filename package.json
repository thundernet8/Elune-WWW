        "compression": "^1.7.0",
        "cookie-parser": "^1.4.3",
        "create-react-class": "^15.6.2",
        "dom-helpers": "^3.2.1",
        "draft-js": "^0.10.2",
        "draftjs-to-html": "^0.7.5",
        "draftjs-to-html-fork": "^0.0.1",
        "ejs": "^2.5.7",
        "element-theme-default": "^1.4.6",
        "express": "^4.15.4",
        "forever": "^0.15.3",
        "immutable": "^3.8.1",
        "mobx": "^3.3.0",
        "mobx-react": "^4.3.3",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-click-outside": "^3.0.0",
        "react-document-meta": "^2.1.2",
        "react-dom": "^16.0.0",
        "react-draft-wysiwyg": "^1.10.12",
        "react-headroom": "^2.1.6",
        "react-router-dom": "^4.2.2",
        "react-transition-group": "^1.2.0",
        "response-time": "^2.3.2",
        "sanitize-html": "^1.14.1",
        "throttle-debounce": "^1.0.1"
    },
    "scripts": {
        "dll": "cross-env NODE_ENV=production webpack -p --hide-modules --config webpack/dll.conf.babel.js",
        "dll:dev": "cross-env NODE_ENV=development webpack --hide-modules --config webpack/dll.conf.babel.js",
        "build": "rimraf dist && mkdirp dist && cross-env NODE_ENV=production npm run dll && webpack -p --progress --hide-modules --config webpack/prod.conf.babel.js",
        "analyze": "rimraf dist && mkdirp dist && cross-env ANALYZE_ENV=true npm run build",
        "build:ssr": "webpack --progress --hide-modules --config webpack/ssr.conf.babel.js && tsc ssr/index.ts",
        "tsc:ssr": "tsc ssr/index.ts",
        "build:dev": "rimraf dist && mkdirp dist && cross-env NODE_ENV=development npm run dll:dev && webpack --progress --hide-modules --config webpack/dev.conf.babel.js",
        "dev": "npm run build:dev && webpack-dev-server --config webpack/dev.conf.babel.js",
        "ssr": "npm run build && npm run build:ssr && node ssr",
        "ssr:dev": "make clean && npm run build:dev && webpack --progress --hide-modules --config webpack/ssr.conf.babel.js && nodemon --watch ssr/**/*.ts --exec ts-node ssr/index.ts",
        "start": "npm run build && npm run build:ssr && forever start ssr/index.js",
        "start:o": "forever start ssr/index.js",
        "tasklist": "forever list",
        "restart": "forever restart ssr/index.js",
        "stop": "forever stop ssr/index.js",
        "lint": "npm run lint:ts && npm run lint:style",
        "lint:ts": "tslint -e node_modules typings -c .tslintrc.json src/**/*.tsx",
        "lint:style": "stylelint \"src/**/*.less\" --syntax less",
        "lint-staged": "lint-staged",
        "lint-staged:ts": "tslint --fix -c .tslintrc.json",
        "lint-staged:style": "stylelint --syntax less",
        "et": "et"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "lint-staged:ts"
        ],
        "src/**/*.less": [
            "lint-staged:style"
        ]
    },
    "pre-commit": "lint-staged",
    "element-theme": {
        "browsers": [
            "ie > 9",
            "last 2 versions"
        ],
        "out": "./src/assets/styles/global/element",
        "config": "./src/assets/styles/element-variables.css",
        "theme": "element-theme-default",
        "minimize": false
    }
}
