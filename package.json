        "tslint": "^5.7.0",
        "tslint-eslint-rules": "^4.1.1",
        "tslint-react": "^3.2.0",
        "typescript": "^2.5.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.5.6",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-server": "^2.7.1"
    },
    "dependencies": {
        "async-validator": "^1.8.1",
        "axios": "^0.16.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "body-parser": "^1.18.1",
